{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/",
      "destination": "/apps/htmlCode/home.html"
    },
    {
      "source": "/htmlCode/:path*",
      "destination": "/apps/htmlCode/:path*"
    },
    {
      "source": "apps/cssCode/:path*",
      "destination": "/apps/cssCode/:path*"
    },
    {
      "source": "apps/jsCode/:path*",
      "destination": "/apps/jsCode/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/apps/assets/:path*"
    }
  ]
}


